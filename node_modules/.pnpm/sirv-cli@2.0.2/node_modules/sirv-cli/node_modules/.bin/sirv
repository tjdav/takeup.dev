#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tom/Projects/dooksa-org/node_modules/.pnpm/sirv-cli@2.0.2/node_modules/sirv-cli/node_modules:/home/tom/Projects/dooksa-org/node_modules/.pnpm/sirv-cli@2.0.2/node_modules:/home/tom/Projects/dooksa-org/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tom/Projects/dooksa-org/node_modules/.pnpm/sirv-cli@2.0.2/node_modules/sirv-cli/node_modules:/home/tom/Projects/dooksa-org/node_modules/.pnpm/sirv-cli@2.0.2/node_modules:/home/tom/Projects/dooksa-org/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin.js" "$@"
else
  exec node  "$basedir/../../bin.js" "$@"
fi
